{
  "name": "genui_server",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "genkit:start": "genkit start -- node lib/index.js",
    "genkit:dev": "LOG_LEVEL=debug genkit start -- tsx src/index.ts",
    "compile": "tsc",
    "build": "pnpm run build:clean && npm run compile",
    "build:clean": "rimraf ./lib",
    "build:watch": "tsc --watch",
    "build-and-run": "pnpm run build && node lib/index.js",
    "serve": "pnpm run build && firebase emulators:start --only genui_server",
    "shell": "pnpm run build && firebase genui_server:shell",
    "start": "pnpm run shell",
    "deploy": "firebase deploy --only genui_server",
    "logs": "firebase genui_server:log",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "keywords": [],
  "author": "Dash team",
  "license": "ISC",
  "description": "Genkit version of GenUI",
  "dependencies": {
    "@genkit-ai/ai": "^1.18.0",
    "@genkit-ai/core": "^1.18.0",
    "@genkit-ai/express": "^1.18.0",
    "@genkit-ai/firebase": "^1.18.0",
    "@genkit-ai/googleai": "^1.18.0",
    "@genkit-ai/vertexai": "^1.18.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "firebase-admin": "^13.5.0",
    "genkit": "^1.18.0",
    "html-entities": "^2.6.0",
    "node": "^20.19.4",
    "node-fetch": "^3.3.2",
    "opentelemetry": "^0.1.0",
    "pino": "^9.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@eslint/json": "^0.13.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/mustache": "^4.2.6",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.57.1",
    "firebase-functions-test": "^3.4.1",
    "genkit": "^1.17.1",
    "genkit-cli": "^1.18.0",
    "globals": "^16.3.0",
    "jest": "^30.1.1",
    "pino-pretty": "^13.1.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "uuid": "^9.0.1"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gspencergoog/genui.git"
  },
  "pnpm": {
    "overrides": {
      "long": "4.0.0"
    }
  }
}
